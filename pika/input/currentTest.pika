func stepForward<int input> -> int {
  	return input + 1.
}

func stepBackward<int input> -> int {
	 return input - 1.
}

func chooseStepFunction<bool backward, int input> -> int {
	if (backward) {
		return stepBackward(input).
	} else {
		return stepForward(input).
	}
}

exec {
	var input := 5.
	print input, _n_.
	
	input := chooseStepFunction(_true_, input).
	print input, _n_.
}