exec {    # Test releasing array    var myArr := [1, 2, 3, 4].    print myArr, _n_.    release myArr.    print myArr, _n_.    # Test releasing multi-dim array    var myArr2 := [[1, 2], [3, 4]].    print length myArr2; ^:^; myArr2, _n_.    release myArr2.    print length myArr2; ^:^; myArr2, _n_.}